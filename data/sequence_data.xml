<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Secuencia para numeración de cotizaciones PERUANITA -->
        <record id="seq_custom_quotation_peruanita" model="ir.sequence">
            <field name="name">Cotización PERUANITA</field>
            <field name="code">sale.order.custom.quotation.peruanita</field>
            <field name="prefix">COTIZACIÓN </field>
            <field name="suffix">-PE</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Secuencia para numeración de cotizaciones GIBBOR -->
        <record id="seq_custom_quotation_gibbor" model="ir.sequence">
            <field name="name">Cotización GIBBOR</field>
            <field name="code">sale.order.custom.quotation.gibbor</field>
            <field name="prefix">COTIZACIÓN </field>
            <field name="suffix">-GIBBOR</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

        <!-- Secuencia para numeración de cotizaciones CORPEALIM -->
        <record id="seq_custom_quotation_corpealim" model="ir.sequence">
            <field name="name">Cotización CORPEALIM</field>
            <field name="code">sale.order.custom.quotation.corpealim</field>
            <field name="prefix">COTIZACIÓN </field>
            <field name="suffix">-CORPEALIM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">standard</field>
        </record>

    </data>
</odoo>